                                        <showHeader>TRUE</showHeader>
                                    </appearance>
                                </treeConfig>
                                <foreign_table>sys_category</foreign_table>
                                <foreign_table_where> AND sys_category.sys_language_uid IN (-1, 0) ORDER BY sys_category.sorting</foreign_table_where>
                                <size>10</size>
                                <autoSizeMax>20</autoSizeMax>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </settings.list.entryNavigationCategory>
                </el>
            </ROOT>
